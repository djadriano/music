<div class="player">
  <div class="player-video-container" ng-show="video.playing != ''">
    <iframe id="player" width="100%" height="300" src="http://www.youtube.com/embed/{{video.playing}}?autoplay=1" frameborder="0"></iframe>
  </div>
  <div class="player-music-container" ng-show="music.playing != ''">
    <iframe width="100%" height="166" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url={{music.playing}}" frameborder="0"></iframe>
  </div>
</div>